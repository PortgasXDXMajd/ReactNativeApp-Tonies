{
  "name": "pendlerratd",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint ./src --ext .ts,.tsx --report-unused-disable-directives",
    "clean": "yarn rimraf node_modules",
    "reset": "yarn clean && yarn && yarn build",
    "bootstrap": "yarn install --frozen-lockfile",
    "bundle:ios:debug": "yarn run react-native bundle --entry-file index.js --platform ios --dev true --bundle-output ios/main.jsbundle --assets-dest ios",
    "bundle:android:debug": "yarn run react-native bundle --entry-file index.js --platform android --dev true --bundle-output ./android/app/src/main/assets/index.android.bundle --assets-dest ./android/app/src/main/res/",
    "bundle:ios:release": "yarn run react-native bundle --entry-file index.js --platform ios --dev false --bundle-output ios/main.jsbundle --assets-dest ios",
    "bundle:android:release": "yarn run react-native bundle --entry-file index.js --platform android --dev false --bundle-output ./android/app/src/main/assets/index.android.bundle --assets-dest ./android/app/src/main/res/",
    "fastlane:ios:debug": "fastlane ios build_debug",
    "fastlane:android:debug": "fastlane android build_debug",
    "fastlane:ios:release": "fastlane ios build_release",
    "fastlane:android:release": "fastlane android build_release",
    "build:ios:debug": "yarn bundle:ios:debug && yarn fastlane:ios:debug",
    "build:android:debug": "yarn jetify && yarn fastlane:android:debug",
    "build:ios:release": "yarn bundle:ios:release && yarn fastlane:ios:release",
    "build:android:release": "yarn jetify && yarn fastlane:android:release",
    "build": "yarn run tsc",
    "pod:install": "pod install --project-directory=./ios/",
    "pod:deintegrate": "pod deintegrate ./ios/*.xcodeproj --project-directory=./ios/",
    "pod:reset": "yarn pod:deintegrate && yarn pod:install",
    "prepare": "husky install"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^1.15.14",
    "@react-native-community/checkbox": "^0.5.9",
    "@react-native-community/cli-tools": "^7.0.1",
    "@react-native-community/geolocation": "^2.0.2",
    "@react-navigation/bottom-tabs": "^6.2.0",
    "@react-navigation/material-top-tabs": "^6.0.6",
    "@react-navigation/native": "^6.0.6",
    "@react-navigation/native-stack": "^6.2.5",
    "@tsdotnet/stopwatch": "^1.0.10",
    "@types/babel__core": "^7.1.18",
    "@types/xml": "^1.0.7",
    "axios": "^0.24.0",
    "bignumber.js": "^9.0.2",
    "geolib": "^3.3.3",
    "gps-to-gpx": "^1.5.1",
    "jwt-decode": "^3.1.2",
    "lottie-react-native": "^5.0.1",
    "moment": "^2.29.1",
    "react": "17.0.2",
    "react-i18next": "^11.14.3",
    "react-native": "0.66.3",
    "react-native-app-auth": "^6.4.0",
    "react-native-background-fetch": "^4.0.4",
    "react-native-background-geolocation": "^4.4.4",
    "react-native-elements": "^3.4.2",
    "react-native-geolocation-service": "^5.3.0-beta.3",
    "react-native-gesture-handler": "^2.2.0",
    "react-native-image-picker": "^4.7.3",
    "react-native-keychain": "^8.0.0",
    "react-native-localization": "^2.1.7",
    "react-native-maps": "0.29.4",
    "react-native-maps-directions": "^1.8.0",
    "react-native-pager-view": "^5.4.9",
    "react-native-progress": "^5.0.0",
    "react-native-reanimated": "^2.4.1",
    "react-native-safe-area-context": "^3.3.2",
    "react-native-screens": "^3.10.0",
    "react-native-sensitive-info": "^6.0.0-alpha.9",
    "react-native-status-bar-height": "^2.6.0",
    "react-native-svg": "^12.1.1",
    "react-native-tab-view": "^3.1.1",
    "react-native-toast-message": "^2.1.0",
    "react-native-typescript-transformer": "^1.2.13",
    "react-native-uuid": "^2.0.1",
    "react-native-vector-icons": "^9.0.0",
    "react-redux": "^7.2.4",
    "redux": "^4.1.1",
    "redux-thunk": "^2.3.0",
    "reflect-metadata": "^0.1.13"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/runtime": "^7.12.5",
    "@commitlint/cli": "^15.0.0",
    "@commitlint/config-conventional": "^15.0.0",
    "@econsor-mobile/auditme": "^1.4.2",
    "@react-native-community/eslint-config": "^3.0.1",
    "@types/jest": "^27.0.3",
    "@types/react-native": "^0.66.8",
    "@types/react-test-renderer": "^17.0.1",
    "@typescript-eslint/eslint-plugin": "^5.5.0",
    "@typescript-eslint/parser": "^5.5.0",
    "babel-jest": "^27.4.2",
    "eslint": "^8.4.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jest": "^25.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.4",
    "jest": "^27.4.3",
    "metro-react-native-babel-preset": "^0.66.2",
    "prettier": "^2.0.5",
    "pretty-quick": "^3.1.2",
    "react-test-renderer": "17.0.2",
    "rimraf": "^3.0.2",
    "typescript": "^4.4.4"
  }
}
